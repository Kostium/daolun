   "source": [
    "import random\n",
    "import math\n",
    "S=2\n",
    "N=100000\n",
    "C=0.0\n",
    "for i in range(N):\n",
    "    x=random.uniform(0.0,1.0)\n",
    "    y=random.uniform(0.0,2.0)\n",
    "    result=math.pow(x,2)+math.pow(x,3)\n",
    "    if(result>=y):\n",
    "        C+=1\n",
    "print(C/N*S)"
   ]

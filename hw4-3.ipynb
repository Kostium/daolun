   "source": [
    "S=input()\n",
    "lens=len(S)\n",
    "i=0\n",
    "max=1\n",
    "sum=1\n",
    "while i<lens-1:\n",
    "    if S[i]==S[i+1]:\n",
    "       sum=sum+1\n",
    "       i=i+1\n",
    "       if sum>max:\n",
    "          max=sum\n",
    "    else:\n",
    "        i=i+1\n",
    "        sum=1\n",
    "print(max)"
   ]
